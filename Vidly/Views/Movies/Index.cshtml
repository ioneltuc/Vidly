@model IEnumerable<Movie>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<table>
    <tr>
        <th>Movies</th>
        <th>Genre</th>
    </tr>
    @foreach (var movie in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(
                    @movie.Name,
                    "Details",
                    "Movies",
                    new { id = movie.Id }
                    )
            </td>
            <td>@movie.Genre</td>
        </tr>
    }
</table>